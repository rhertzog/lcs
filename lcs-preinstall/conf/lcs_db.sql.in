--
-- Structure de la table `personne`
--

CREATE TABLE IF NOT EXISTS `personne` (
  `id` smallint(5) NOT NULL auto_increment,
  `last_log` varchar(14) default NULL,
  `act_log` varchar(14) default NULL,
  `login` varchar(60) NOT NULL default '',
  `stat` smallint(6) NOT NULL default '0',
  PRIMARY KEY  (`id`)
) TYPE=MyISAM AUTO_INCREMENT=1 ;


-- --------------------------------------------------------

--
-- Structure de la table `sessions`
--

CREATE TABLE IF NOT EXISTS `sessions` (
  `id` smallint(5) unsigned NOT NULL auto_increment,
  `sess` varchar(20) NOT NULL default '',
  `mdp` varchar(20) NOT NULL default '0',
  `idpers` smallint(5) unsigned NOT NULL default '0',
  `remote_ip` varchar(32) default NULL,
  PRIMARY KEY  (`id`),
  UNIQUE KEY `id_2` (`id`,`sess`),
  KEY `id` (`id`,`sess`)
) TYPE=MyISAM AUTO_INCREMENT=1 ;


-- --------------------------------------------------------

--
-- Structure de la table `statusages`
--

CREATE TABLE IF NOT EXISTS `statusages` (
  `grp` varchar(15) NOT NULL default '',
  `appli` varchar(20) NOT NULL default '',
  `timestamp` varchar(14) NOT NULL default '',
  `source` char(3) NOT NULL default '',
  `login` varchar(30) NOT NULL
) ENGINE=MyISAM DEFAULT CHARSET=latin1;

-- --------------------------------------------------------


--
-- Structure de la table `redirmail`
--

CREATE TABLE IF NOT EXISTS `redirmail` (
  `id` smallint(6) NOT NULL auto_increment,
  `faitpar` text NOT NULL,
  `pour` text NOT NULL,
  `vers` text NOT NULL,
  `copie` varchar(3) NOT NULL,
  `date` timestamp NOT NULL default CURRENT_TIMESTAMP,
  `remote_ip` text NOT NULL,
  PRIMARY KEY  (`id`)
) ENGINE=MyISAM  DEFAULT CHARSET=latin1  ;

