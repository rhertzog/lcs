 CREATE TABLE IF NOT EXISTS `sequences` (
  `id_seq` mediumint(8) unsigned NOT NULL auto_increment,
  `id_ong` mediumint(9) NOT NULL,
  `titre` varchar(128) NOT NULL,
  `titrecourt` varchar(20) NOT NULL,
